
<form id="BoostCakeBootstrap3Form"  accept-charset="utf-8" method="post" action="/boost_cake/boost_cake/bootstrap3">
  <div class="col-xs-12 col-sm-12 col-md-12">
    <div class="col-xs-12 col-sm-12 col-md-12">
      <h3>Billing Information</h3>
    </div>
  </div>
  <pre><%= @contact.inspect %></pre>
  <div class="col-xs-12 col-sm-12 col-md-12">

      <div class="col-xs-12 col-sm-6 col-md-6">
        <div class="form-group">
          <label for="BoostCakeText">First Name</label>
          <input type="text" id="BoostCakeText" class="form-control" name="data[BoostCake][text]">   
        </div>
        <div class="form-group">
          <label for="">Last Name</label>
          <input type="text" id="" class="form-control" name="data[user][test]">
        </div>
        <div class="form-group">
          <label for="">Company Name(optional)</label>
          <input type="text" id="" class="form-control" name="data[user][test]">
        </div>
        <div class="form-group">
          <label for="">Phone Number</label>
          <input type="text" id="" class="form-control" name="data[user][test]">
        </div>
        <div class="form-group">
          <label for="">Email Address</label>
          <input type="text" id="" class="form-control" name="data[user][test]">
        </div>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6">
        <div class="form-group">    
          <label for="BoostCakeText">Billing Address</label>
          <input type="text" id="BoostCakeText" class="form-control" name="data[BoostCake][text]">   
        </div>
        <div class="form-group">
          <label for="">City</label>
          <input type="text" id="" class="form-control" name="data[user][test]">
        </div>
        
        <div class="col-xs-12 col-sm-6 col-md-6">
          <div class="form-group">
            <label>State</label>
            <select class="form-control">
              <option>Select State</option>
              <option value="PA">Pennsylvania</option>
              <option value="CT">Connecticut</option>
              <option value="NY">New York</option>
              <option value="MD">Maryland</option>
              <option value="VA">Virginia</option>
            </select>          
          </div>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-6">
          <div class="form-group">
            <label>Postal Code</label>
            <input type="text" id="" class="form-control" name="data[user][test]">
          </div>
        </div>        
        <div class="form-group">
        
          <label>Name on Credit Card</label>
          <input type="text" class="form-control" name="creditcard">

        </div>

        <div class="form-group">
          <label>Credit Card Number</label>
          <input type="text" class="form-control" name="cardnumber">
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12">      
          <div class="col-xs-8 col-sm-8 col-md-8">
            <label>Expiration Date</label>
          </div>
          <div class="col-xs-4 col-sm-4 col-md-4">
            <label>CVV</label>
          </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12">      
          <div class="col-xs-4 col-sm-4 col-md-4">
            <div class="form-group">
              <select class="form-control">
                <option>Month</option>
                <option value="PA">1</option>
                <option value="CT">2</option>
                <option value="NY">3</option>
                <option value="MD">4</option>
                <option value="VA">5</option>
              </select>
            </div>
          </div>
          <div class="col-xs-4 col-sm-4 col-md-4">
            <select class="form-control">
              <option>Year</option>
              <option value="PA">2014</option>
              <option value="CT">2015</option>
              <option value="NY">2016</option>
              <option value="MD">2017</option>
              <option value="VA">2018</option>
            </select>   
          </div>          
          <div class="col-xs-4 col-sm-4 col-md-4">
            <div class="form-group">              
              <input type="text" class="form-control" name="cvv">              
            </div>
          </div>
        </div>
      </div>
  </div>

  <div class="col-xs-12 col-sm-12 col-md-12">
    
    <div class="col-xs-12 col-sm-12 col-md-6"> 
      <div class="col-xs-12 col-sm-12 col-md-12 well"> 
        <div class="form-group">
          <h3>Order Summary</h3>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-6">
          <div class="form-group">
            <label>Plan</label>            
          </div>
          <div class="form-group">
            <label>Monthly</label>
          </div>
        </div>
        <div class="col-xs-6 col-sm-6 col-md-6">
          <div class="form-group">
            <label>CHARGE</label>            
          </div>
          <div class="form-group">
            <label>$47 Each Month</label>
          </div>
        </div>
      </div>
    </div>

    <div class="col-xs-12 col-sm-12 col-md-6">
      <div class="form-group">
        <button class="btn btn-order form-control"> PLACE MY ORDER </button>          
      </div>
      <div class="form-group">
        <p class="well">By clicking "PLACE MY ORDER" you agree to our <a href="#">Terms and Conditions</a></p>
      </div>
    </div>
  </div>


  <div class="col-xs-12 col-sm-12 col-md-6">
    <div class="col-xs-12 col-sm-12 col-md-6">
      <a href="#">Have a coupon?</a>
    </div>
    <div class="col-xs-8 col-sm-8 col-md-8">      
      <div class="form-group">
        <input type="text" class="form-control" />
      </div>      
    </div>
    <div class="col-xs-4 col-sm-4 col-md-4">
      <button class="btn btn-coupon-apply form-control"> APPLY </button>          
    </div>
  </div>

</form>



